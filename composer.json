{
    "name": "rebet/rebet",
    "description": "The Rebet PHP framework",
    "keywords": [
        "framework",
        "rebet"
    ],
    "authors": [
        {
            "name": "Junichi Shibata",
            "email": "rain.noise@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "support": {
        "email": "rain.noise@gmail.com",
        "issues": "https://github.com/rebet/rebet/issues",
        "source": "https://github.com/rebet/rebet",
        "docs": "https://github.com/rebet/rebet/blob/master/README.md"
    },
    "require": {
        "php": "^7.3",
        "doctrine/annotations": "^1.10",
        "greenlion/php-sql-parser": "^4.3",
        "html2text/html2text": "^4.3",
        "league/flysystem": "^1.1",
        "league/flysystem-cached-adapter": "^1.1",
        "monolog/monolog": "^2.1",
        "piwik/device-detector": "^3.13",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/cache": "^5.1",
        "symfony/console": "^5.1",
        "symfony/http-foundation": "^5.1",
        "symfony/mime": "^5.1",
        "vlucas/phpdotenv": "^5.1"
    },
    "require-dev": {
        "illuminate/view": "^7.27",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-memory": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.3",
        "predis/predis": "^1.1",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Rebet\\": "src/Rebet/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rebet\\Tests\\": "tests/src/Rebet/Tests/"
        }
    },
    "suggest": {
        "illuminate/view": "Required to use the 'Blade' view template engine (^5.7).",
        "twig/twig": "Required to use the 'Twig' view template engine (^3.1).",
        "league/flysystem-aws-s3-v3": "Required to use the Flysystem S3 driver (^1.0).",
        "league/flysystem-sftp": "Required to use the Flysystem SFTP driver (^1.0)."
    },
    "config": {
        "sort-packages": true
    }
}

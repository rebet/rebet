{
    "name": "rebet/rebet",
    "description": "The Rebet PHP framework",
    "keywords": [
        "framework",
        "rebet"
    ],
    "authors": [
        {
            "name": "Junichi Shibata",
            "email": "rain.noise@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "support": {
        "email": "rain.noise@gmail.com",
        "issues": "https://github.com/rebet/rebet/issues",
        "source": "https://github.com/rebet/rebet",
        "docs": "https://github.com/rebet/rebet/blob/master/README.md"
    },
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.6",
        "greenlion/php-sql-parser": "^4.3",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "monolog/monolog": "^1.24",
        "piwik/device-detector": "^3.11",
        "symfony/console": "^4.4",
        "symfony/http-foundation": "^4.1",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "illuminate/view": "^5.7",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-memory": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/dbunit": "^4.0",
        "phpunit/phpunit": "^7.3.0",
        "twig/twig": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Rebet\\": "src/Rebet/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rebet\\Tests\\": "tests/Rebet/Tests/"
        }
    },
    "suggest": {
        "illuminate/view": "Required to use the 'Blade' view template engine (^5.7).",
        "twig/twig": "Required to use the 'Twig' view template engine (^3.1).",
        "league/flysystem-aws-s3-v3": "Required to use the Flysystem S3 driver (^1.0).",
        "league/flysystem-sftp": "Required to use the Flysystem SFTP driver (^1.0)."
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "bin/rebet"
    ]
}
